{
  "name": "tableflip",
  "version": "0.0.0",
  "homepage": "https://table-flip.de",
  "private": true,
  "type": "module",
  "scripts": {
    "deploy": "npm run clear-gh-pages-cache  && gh-pages -d dist -b gh_tableflip --cname table-flip.de -r https://github.com/PhilippBraml/table-flip-homepage.git",
    "clear-gh-pages-cache": "rimraf node_modules/.cache/gh-pages",
    "start": "vite",
    "build": "npm run type-check & npm run build-only -- && cp dist/index.html dist/404.html",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@vicons/ionicons5": "^0.13.0",
    "@vicons/utils": "^0.1.4",
    "@vue-leaflet/vue-leaflet": "^0.10.1",
    "@vueuse/core": "^10.4.0",
    "leaflet": "^1.9.4",
    "naive-ui": "^2.42.0",
    "pinia": "^3.0.1",
    "vfonts": "^0.0.3",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.11",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/leaflet": "1.9.18",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.22.0",
    "eslint-plugin-vue": "~10.0.0",
    "gh-pages": "^6.3.0",
    "jiti": "2.6.1",
    "jsdom": "^26.0.0",
    "prettier": "3.6.2",
    "rimraf": "6.0.1",
    "typescript": "~5.8.0",
    "unplugin-vue-router": "^0.12.0",
    "vite": "^6.2.4",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vue-tsc": "^2.2.8"
  }
}